FoamFile
{
    format      ascii;
    class       volScalarField;
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "(jet_inlet|inlet|outlet|top|bottom|wall_inlet|left_side|right_side)"
    {
        type     calculated;
	    value    $internalField;
    }
}

