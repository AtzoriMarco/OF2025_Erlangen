FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.air       1;
    alpha.water     0;
}

zones
{
    water
    {
        type        sphere;
        zoneType    cell;

        centre      (0.005 0.005 0); 
    	radius 	     0.002;

        values
        {
            alpha.water     1;
            alpha.air       0;
        }
    }

}


// ************************************************************************* //
