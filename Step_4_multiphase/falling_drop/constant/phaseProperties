FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases          (water air);

water
{
    type            pureIsothermalPhaseModel;

    diameterModel   none;

    residualAlpha   1e-3;
}

air
{
    type            pureIsothermalPhaseModel;

    diameterModel   none;

    residualAlpha   1e-3;
}

blending
{
    default
    {
        type            segregated;
    }
}

surfaceTension
{
    air_water
    {
        type            constant;
        sigma           0.07;
    }

}

interfaceCompression
{
    air_water       1;
}

