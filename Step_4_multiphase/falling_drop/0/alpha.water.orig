FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    movingWall
    {
        type            alphaContactAngle;
        contactAngleProperties
        {
            air             { theta0 90; }
        }
        value           $internalField;
    }

    fixedWalls
    {
        type            alphaContactAngle;
        contactAngleProperties
        {
            air             { theta0 90; }
        }
        value           $internalField;
    }

    frontAndBack
    {
        type            empty;
    }
}


